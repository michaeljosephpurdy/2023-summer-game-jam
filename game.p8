--timed function
timed_function={
 fns={},
 update=function(dt)
  foreach(timed_function.fns,function(o)
   -- o[1] is time
   log(o)
   o[1]-=dt
   if o[1]<=0 then
    -- o[2] is fn
    o[2]()
    del(timed_function.fns,o)
   end
  end)
 end,
 new=function(fn,t)
  local o={t,fn}
  add(timed_function.fns,o)
 end
}
